						<title><xsl:value-of select="title"/></title>
						<link><xsl:value-of select="link"/></link>
						<content><xsl:value-of select="description"/></content>
						<id><xsl:value-of select="guid"/></id>
						<updated>2002-10-02T10:00:00-05:00</updated>
					</entry>
				</xsl:for-each>
			</xsl:template>
		</xsl:stylesheet>
	</feed>

